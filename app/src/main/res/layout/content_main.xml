<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_main"
        tools:context=".activity.MainActivity">

    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/frameLayout" tools:layout_editor_absoluteY="0dp" tools:layout_editor_absoluteX="44dp">
        <TextView
                android:text="@string/mensajePrincipal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView"
                tools:layout_width="wrap_content" android:layout_alignParentStart="true"
                android:layout_marginStart="120dp" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp"/>
    </RelativeLayout>




</android.support.constraint.ConstraintLayout>